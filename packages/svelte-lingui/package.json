{
	"name": "@mikstack/svelte-lingui",
	"version": "0.3.0",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/MikaelSiidorow/mikstack.git",
		"directory": "packages/svelte-lingui"
	},
	"scripts": {
		"prepare": "svelte-kit sync || echo ''",
		"build": "svelte-kit sync && svelte-package && publint && attw --pack . --profile esm-only --ignore-rules internal-resolution-error",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "vitest --run",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./extractor": {
			"types": "./dist/extractor/index.d.ts",
			"default": "./dist/extractor/index.js"
		},
		"./vite": {
			"types": "./dist/vite/index.d.ts",
			"default": "./dist/vite/index.js"
		},
		"./preprocessor": {
			"types": "./dist/preprocessor/index.d.ts",
			"default": "./dist/preprocessor/index.js"
		}
	},
	"peerDependencies": {
		"@lingui/core": ">=5.0.0",
		"svelte": "^5.0.0"
	},
	"dependencies": {
		"@lingui/message-utils": "^5.0.0",
		"gettext-parser": "^8.0.0",
		"magic-string": "^0.30.21"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.18.2",
		"@eslint/compat": "^2.0.1",
		"@eslint/js": "^9.39.2",
		"@lingui/core": "^5.0.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.50.1",
		"@sveltejs/package": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@types/node": "^22",
		"@typescript-eslint/typescript-estree": "^8.0.0",
		"@vitest/browser-playwright": "^4.0.18",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.14.0",
		"estree-walker": "^3.0.3",
		"globals": "^17.1.0",
		"playwright": "^1.58.0",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"publint": "^0.3.17",
		"svelte": "^5.49.0",
		"svelte-check": "^4.3.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.54.0",
		"vite": "^7.3.1",
		"vitest": "^4.0.18",
		"vitest-browser-svelte": "^2.0.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"svelte",
		"i18n",
		"lingui",
		"internationalization"
	]
}
