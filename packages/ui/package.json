{
  "name": "@mikstack/ui",
  "version": "0.3.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MikaelSiidorow/mikstack.git",
    "directory": "packages/ui"
  },
  "scripts": {
    "prepare": "svelte-kit sync || echo ''",
    "build": "svelte-kit sync && svelte-package && publint && attw --pack . --profile esm-only --ignore-rules internal-resolution-error",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "exports": {
    "./Accordion": {
      "types": "./dist/Accordion/index.d.ts",
      "svelte": "./dist/Accordion/index.js"
    },
    "./Alert": {
      "types": "./dist/Alert/index.d.ts",
      "svelte": "./dist/Alert/index.js"
    },
    "./Badge": {
      "types": "./dist/Badge/index.d.ts",
      "svelte": "./dist/Badge/index.js"
    },
    "./Button": {
      "types": "./dist/Button/index.d.ts",
      "svelte": "./dist/Button/index.js"
    },
    "./Card": {
      "types": "./dist/Card/index.d.ts",
      "svelte": "./dist/Card/index.js"
    },
    "./Dialog": {
      "types": "./dist/Dialog/index.d.ts",
      "svelte": "./dist/Dialog/index.js"
    },
    "./FormField": {
      "types": "./dist/FormField/index.d.ts",
      "svelte": "./dist/FormField/index.js"
    },
    "./Input": {
      "types": "./dist/Input/index.d.ts",
      "svelte": "./dist/Input/index.js"
    },
    "./Separator": {
      "types": "./dist/Separator/index.d.ts",
      "svelte": "./dist/Separator/index.js"
    },
    "./Skeleton": {
      "types": "./dist/Skeleton/index.d.ts",
      "svelte": "./dist/Skeleton/index.js"
    },
    "./Switch": {
      "types": "./dist/Switch/index.d.ts",
      "svelte": "./dist/Switch/index.js"
    },
    "./Textarea": {
      "types": "./dist/Textarea/index.d.ts",
      "svelte": "./dist/Textarea/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.50.1",
    "@sveltejs/package": "^2.5.7",
    "publint": "^0.3.17",
    "svelte": "^5.49.0",
    "svelte-check": "^4.3.6",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "svelte",
    "ui",
    "components",
    "css"
  ]
}
